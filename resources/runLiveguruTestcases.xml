<?xml version="1.0" encoding="UTF-8"?>
<suite name="hybrid-framework-liveguru" parallel="false">
  
  <parameter name="url" value="http://live.demoguru99.com/" />
  
  <test name="run on Firefox">
  <parameter name="browser" value="firefox" />
    <classes>
      <class name="com.liveguru.login.Level_04_Register_Login_Multiple_Browser"/>
    </classes>
  </test>
  
  <test name="run on Chrome">
  <parameter name="browser" value="chrome" />
    <classes>
      <class name="com.liveguru.login.Level_04_Register_Login_Multiple_Browser"/>
    </classes>
  </test>
   
  <test name="run on Edge Chromium">
  <parameter name="browser" value="edge_chromium" />
    <classes>
      <class name="com.liveguru.login.Level_04_Register_Login_Multiple_Browser"/>
    </classes>
  </test>
   
</suite>
